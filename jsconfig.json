{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@components/*": ["components/*"],
            "@assets/*": ["assets/*"],
            "@atoms/*": ["components/atoms/*"],
            "@molecules/*": ["components/molecules/*"],
            "@organisms/*": ["components/organisms/*"],
            "@routes/*": ["routes/*"],
            "@pages/*": ["pages/*"],
